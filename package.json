{
	"name": "4dn-microscopy-metadata-tool-omero",
	"version": "0.0.21-1",
	"private": true,
	"main": "./main.js",
	"publishConfig": {
		"access": "restricted"
	},
	"dependencies": {
		"4dn-microscopy-metadata-tool": "github:WU-BIMAC/4DNMicroscopyMetadataToolReact#master",
		"react": "^16.11.0",
		"react-bootstrap": "^1.0.0-beta.14",
		"react-dom": "^16.11.0"
	},
	"scripts": {
		"test": "node scripts/test.js",
		"buildProd": "gulp build-prod",
		"buildDev": "gulp build-dev",
		"dev": "gulp dev",
		"build": "gulp build-prod && gulp build-dev",
		"update-mma": "npm uninstall 4dn-microscopy-metadata-tool && npm cache clean --force && npm install 4dn-microscopy-metadata-tool@github:WU-BIMAC/4DNMicroscopyMetadataToolReact#master --save"
	},
	"build": {},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@babel/core": "^7.6.4",
		"@babel/preset-env": "^7.6.3",
		"@babel/preset-react": "^7.6.3",
		"babel-eslint": "^10.0.3",
		"babel-loader": "^8.0.6",
		"babel-preset-react-app": "^9.0.2",
		"case-sensitive-paths-webpack-plugin": "2.2.0",
		"css-loader": "2.1.1",
		"dotenv": "8.0.0",
		"dotenv-expand": "5.1.0",
		"eslint": "5.16.0",
		"eslint-loader": "2.1.2",
		"eslint-plugin-flowtype": "3.9.1",
		"eslint-plugin-import": "2.17.3",
		"eslint-plugin-jsx-a11y": "6.2.1",
		"eslint-plugin-react": "7.13.0",
		"fancy-log": "^1.3.3",
		"fs": "0.0.1-security",
		"gulp": "^4.0.2",
		"html-webpack-plugin": "4.0.0-alpha.2",
		"jest": "24.8.0",
		"jest-pnp-resolver": "1.2.1",
		"jest-resolve": "24.8.0",
		"jest-watch-typeahead": "^0.3.1",
		"mini-css-extract-plugin": "0.7.0",
		"optimize-css-assets-webpack-plugin": "5.0.1",
		"plugin-error": "^1.0.1",
		"pnp-webpack-plugin": "1.4.3",
		"postcss-flexbugs-fixes": "4.1.0",
		"postcss-loader": "3.0.0",
		"postcss-preset-env": "6.6.0",
		"postcss-safe-parser": "4.0.1",
		"react-dev-utils": "^9.1.0",
		"rm-local-modules": "0.0.2",
		"sass-loader": "7.1.0",
		"style-loader": "0.23.1",
		"typescript": "^3.6.4",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.9",
		"workbox-webpack-plugin": "4.3.1"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}",
			"!src/**/*.d.ts"
		],
		"resolver": "jest-pnp-resolver",
		"setupFiles": [
			"react-app-polyfill/jsdom"
		],
		"testMatch": [
			"<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
			"<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
		],
		"testEnvironment": "jsdom",
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
			"^.+\\.module\\.(css|sass|scss)$"
		],
		"moduleNameMapper": {
			"^react-native$": "react-native-web",
			"^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
		},
		"moduleFileExtensions": [
			"web.js",
			"js",
			"web.ts",
			"ts",
			"web.tsx",
			"tsx",
			"json",
			"web.jsx",
			"jsx",
			"node"
		]
	},
	"babel": {
		"presets": [
			"react-app"
		]
	}
}
